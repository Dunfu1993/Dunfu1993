    "def f(s):\n",
    "    return 'a' in s\n",
    "\n",
    "def satisfiesF(L):\n",
    "    for i, ele in enumerate(L):\n",
    "        if not f(ele):\n",
    "            L.pop(i)\n",
    "    return len(L)"
